
<div class="page-header">
	<h1>Problems</h1>
</div>

<table class="table table-striped">
  <thead>
  	<tr>
  		<th>Problem ID</th>
  		<th>Title</th>
  		<th>Info</th>
  		<th>AC</th>
  		<th>Submit</th>
  		<th>Ratio</th>
  	</tr>
  </thead>
  <tbody>
  	<% @problems.each do |problem| %>
  		<tr>
  			<td><%= problem.id %></td>
  			<td><%= link_to problem.title, problem_path(problem)%></td>
  			<td></td>
  			<td></td>
  			<td></td>
  			<td></td>
  		</tr>
  	<% end %>
  </tbody>
</table>
